using System;

namespace ifelseif
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 10, b = 50, c = 30;

            //using if statement to check the alphabet which has higher value

            if (a > b && a > c) 
            {
                Console.WriteLine(" a is gratest");
            }
            else if (b > c && b > a) 
            {
                Console.WriteLine("b is gratest");
            }
            else
            {
                Console.WriteLine("c is gratest");
            }

            Console.ReadKey();
        }
    }
}
