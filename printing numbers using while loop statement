using System;
namespace whileloop
{
    class Program
    {
        static void Main(string[] args)
        {
            int number;
            Console.WriteLine("Please enter the number to knows it's multiplication table");
            number = Convert.ToInt32(Console.ReadLine());

            int x = 1;
            
            while(x <= 10)
            {
                Console.WriteLine(number + "X" + x + "=" + number*x);
                x++;
            }

            Console.ReadKey();

        }
    }
}
